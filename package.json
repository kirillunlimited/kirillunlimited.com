{
  "name": "kirillunlimited.ru",
  "version": "1.0.0",
  "scripts": {
    "clean": "npm-run-all --parallel clean:*",
    "clean:cache": "npx shx rm -rf .parcel-cache",
    "clean:dist": "npx shx rm -rf dist",
    "dev": "npm-run-all clean --parallel dev:*",
    "dev:eleventy": "eleventy --watch --quiet",
    "dev:parcel": "parcel watch dist/tmp/**/*.html --dist-dir dist/public",
    "dev:firebase": "firebase emulators:start",
    "build": "npm-run-all clean build:*",
    "build:eleventy": "eleventy",
    "build:parcel": "parcel build --dist-dir dist/public",
    "deploy": "npm-run-all build deploy:*",
    "deploy:firebase": "firebase deploy"
  },
  "author": "Kirill Ivanov",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^9.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "prettier": "^2.3.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ],
  "dependencies": {
    "markdown-it": "^12.0.6",
    "markdown-it-attrs": "^4.0.0"
  },
  "source": "dist/tmp/index.html"
}
